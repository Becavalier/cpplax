class Doughnut {
  cook() {
    print("Dunk in the fryer.");
    this.finish("sprinkles");
  }

  finish(ingredient) {
    print("Finish with " + ingredient + this.a);
  }
}

class Cruller < Doughnut {
  init() {
    this.a = "!23";
  }
  finish(ingredient) {
    // No sprinkles, always icing.
    super.finish("icing");
  }
}

Cruller().finish("adsf");