class Foo {
  init(arg) {
    print("Foo.init(" + arg + ")");
    this.field = "init";
  }
}

var foo = Foo("one");  // "Foo.init(one)".
foo.field = "field";

var foo2 = foo.init("two");  // "Foo.init(two)".
print(foo2);  // "<instance Foo>".

// Make sure init() doesn't create a fresh instance.
print(foo.field);  // "init".
